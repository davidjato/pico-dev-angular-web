{
  "name": "pico-dev-web-angular",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "node dist/pico-dev-web-angular/server/server.mjs",
    "dev": "ng serve",
    "build": "ng build",
    "build:ssr": "ng build --configuration production",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "serve:ssr:pico-dev-web-angular": "node dist/pico-dev-web-angular/server/server.mjs",
    "heroku-postbuild": "ng build --configuration production"
  },
  "engines": {
    "node": "20.x",
    "npm": "10.x"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/build": "^20.3.7",
    "@angular/cli": "^20.3.7",
    "@angular/compiler-cli": "^20.3.0",
    "@angular/common": "^20.3.0",
    "@angular/compiler": "^20.3.0",
    "@angular/core": "^20.3.0",
    "@angular/forms": "^20.3.0",
    "@angular/platform-browser": "^20.3.0",
    "@angular/platform-server": "^20.3.0",
    "@angular/router": "^20.3.0",
    "@angular/ssr": "^20.3.7",
    "@ngx-translate/core": "^17.0.0",
    "@ngx-translate/http-loader": "^17.0.0",
    "angular-three": "^2.10.0",
    "angular-three-postprocessing": "^2.14.0",
    "express": "^5.1.0",
    "maath": "^0.10.8",
    "ngxtension": "^7.0.2",
    "postprocessing": "^6.38.0",
    "rxjs": "~7.8.0",
    "three": "^0.180.0",
    "three-stdlib": "^2.36.1",
    "tslib": "^2.3.0",
    "tweakpane": "^4.0.5",
    "typescript": "~5.9.2"
  },
  "devDependencies": {
    "@types/express": "^5.0.1",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^20.17.19",
    "@types/three": "^0.180.0",
    "jasmine-core": "~5.9.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0"
  }
}
